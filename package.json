{
  "name": "afterstrife-launcher",
  "description": "AF Launcher",
  "author": "Robert Veltman",
  "homepage": "./",
  "version": "0.1.22",
  "private": false,
  "license": "MIT",
  "build": {
    "appId": "com.github.robb-veltman.af-launcher",
    "files": [
      "./build/**/*",
      "./public/electron.js",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "af-launcher",
      "owner": "robb-veltman"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@mdi/js": "^6.5.95",
    "@mdi/react": "^1.5.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/classnames": "^2.3.1",
    "classnames": "^2.3.1",
    "compare-versions": "^4.1.3",
    "decompress-zip": "^0.3.3",
    "electron-dl": "^3.3.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.4",
    "electron-settings": "^4.0.2",
    "electron-updater": "^4.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robb-veltman/af-launcher.git"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"yarn start\" \"yarn:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "build-electron": "yarn build && electron-builder build --win --publish never",
    "publish": "yarn build && electron-builder build --win --publish always"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browser": {
    "fs": true
  },
  "resolutions": {
    "mini-css-extract-plugin": "2.4.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^17.0.38",
    "concurrently": "^7.0.0",
    "electron": "^16.0.7",
    "electron-builder": "^22.14.5",
    "typescript": "^4.5.4",
    "wait-on": "^6.0.0"
  }
}
